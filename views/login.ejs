<!doctype html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="/css_login/bootstrap.min.css">
    <link rel="stylesheet" href="/css_login/fontawesome.min.css">
    <link rel="stylesheet" href="/css_login/jquery-ui.css">
    <link rel="stylesheet" href="/cscss_login/plugin/nice-select.css">
    <link rel="stylesheet" href="/css_login/plugin/magnific-popup.css">
    <link rel="stylesheet" href="/css_login/plugin/slick.css">
    <link rel="stylesheet" href="/css_login/arafat-font.css">
    <link rel="stylesheet" href="/css_login/plugin/animate.css">
    <link rel="stylesheet" href="/css_login/style.css">
</head>

<body>
    <!-- start preloader -->
    <div class="preloader" id="preloader"></div>
    <!-- end preloader -->

    <!-- Login Reg start -->
    <section class="login-reg">
        <div class="overlay pt-120">
            <div class="container">
                <div class="row align-items-center justify-content-center">
                    <div class="col-xl-6 order-xl-0 order-1">
                    
                    </div>
                    <div class="col-xl-5">
                        <form action="/login/login" method="post" style="margin-top: 500px;">
                            <div class="row">
                                <div class="col-12">
                                    <h3>Login into your account</h3>
                                    <p>If you dont have an account : <br><br><a href="/signup/signup" class="cmn-btn">Register</a></p><br>
                                    <div class="single-input">
                                        <label for="logemail">Your Email</label>
                                        <input type="text" id="email" name="email" placeholder="Enter Your Email">
                                    </div>
                                    <div class="single-input">
                                        <label for="logpassword">Your Password</label>
                                        <input type="password" id="password" name="password" placeholder="Enter Your Password">
                                    </div>
                                    <button class="cmn-btn w-100">Submit</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Login Reg end -->

    <!--==================================================================-->
    <script src="/js_login/jquery.min.js"></script>
    <script src="/js_login/jquery-ui.js"></script>
    <script src="/js_login/bootstrap.min.js"></script>
    <script src="/js_login/fontawesome.js"></script>
    <script src="/js_login/plugin/slick.js"></script>
    <script src="/js_login/plugin/jquery.nice-select.min.js"></script>
    <script src="/js_login/plugin/counter.js"></script>
    <script src="/js_login/plugin/waypoint.min.js"></script>
    <script src="/js_login/plugin/jquery.magnific-popup.min.js"></script>
    <script src="/js_login/plugin/wow.min.js"></script>
    <script src="/js_login/plugin/plugin.js"></script>
    <script src="/js_login/main.js"></script>
</body>

</html>